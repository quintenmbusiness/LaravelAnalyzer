{
  "name": "quintenmbusiness/laravel-analyzer",
  "description": "Laravel Analyzer is a developer-focused package that inspects your Laravel application and extracts structured metadata about your codebase.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Quinten Muijser",
      "email": "quintenm.business@outlook.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^11.0 | ^12.0",
    "laravel/framework": "^11.0 | ^12.0",
    "nikic/php-parser": "^5.7"
  },
  "extra": {
    "laravel": {
      "providers": [
        "quintenmbusiness\\LaravelAnalyzer\\Laravel\\app\\Providers\\LaravelAnalyzerServiceProvider"
      ]
    }
  },
"autoload": {
    "psr-4": {
      "quintenmbusiness\\LaravelAnalyzer\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
